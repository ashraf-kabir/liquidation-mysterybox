{
	"description": "Manaknight Code Builder",
	"name": "manaknightdigital/code_builder",
	"type": "project",
	"version": "1.0.0",
	"homepage": "https://manaknightdigital.com",
	"license": "Proprietary Software licensing",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": "^7.3",
		"stripe/stripe-php": "^7.25",
		"mailgun/mailgun-php": "^2.8",
		"php-http/curl-client": "^1.7",
		"guzzlehttp/psr7": "^1.4",
		"firebase/php-jwt": "^5.0",
		"google/apiclient": "^2.0",
		"robmorgan/phinx": "^0.9.2",
		"aws/aws-sdk-php": "^3.104",
		"twilio/sdk": "^5.31",
		"dompdf/dompdf": "^0.8.3",
		"facebook/graph-sdk": "^5.7",
		"picqer/php-barcode-generator": "^2.0",
		"ext-json": "*",
		"ext-intl": "*",
		"ext-zlib": "*",
		"mike42/gfx-php" : "^0.6",
		"box/spout": "^3.1",
		"http-interop/http-factory-guzzle": "^1.2"
	},
	"require-dev": {
		"phpunit/phpunit" : "^6.5",
		"squizlabs/php_codesniffer" : "^3.3"
	},
	"autoload" : {
		"psr-4" : {
			"Mike42\\" : "/../application/libraries/printer/src/Mike42"
		}
	},
	"config": {
		"allow-plugins": {
			"php-http/discovery": true
		}
	}
}
